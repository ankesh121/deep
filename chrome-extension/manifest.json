{
  "manifest_version": 2,

  "name": "DEEP: Create Lead",
  "description": "Submit a lead from current tab to DEEP",
  "version": "1.0",

  "browser_action": {
    "default_icon": "deep-logo.png",
    "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },
  "permissions": [
    "activeTab",
    "http://localhost:8000/*",
    "<all_urls>"
  ]
}
