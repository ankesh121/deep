{
    "manifest_version": 2,

    "name": "DEEP: Add Lead",
    "description": "Submit a lead from current tab to the DEEP",
    "version": "2.0.2",

    "browser_action": {
        "default_icon": "img/deep-logo.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },

    "options_page": "options.html",

    "permissions": ["activeTab", "<all_urls>", "storage"]
}
